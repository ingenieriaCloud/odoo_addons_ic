  <template id="ode_etiqueta_plantilla_informe">
    <t t-call="report.html_container">
      <t t-foreach="doc_ids" t-as="o">
            <t t-raw="translate_doc(o, doc_model, 'partner_id.lang', 'ode_custom.ode_etiqueta_plantilla_document')"/>
        </t>
    </t>
</template>
  
</data>
</openerp>
