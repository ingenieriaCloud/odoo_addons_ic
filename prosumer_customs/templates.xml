<openerp>
    <data>

        <template id="website_layout_custom" inherit_id="website.layout">
            <xpath expr="//div[@class='navbar navbar-default navbar-static-top']" position="attributes">
                 <attribute name="style">background: #444</attribute>
            </xpath>
            <xpath expr="//footer" position="attributes">
                 <attribute name="style">background: #444</attribute>
            </xpath>
        </template>

         <template id="website_layout_custom_footer" inherit_id="website.footer_custom">
            <xpath expr="//footer/div" position="replace">
                <div class="container hidden-print">
                  <div class="row">
                    <div class="col-md-3">
                      <h4>
                        <img src="/logo.png" alt="Logo of Prosumer S.L." title="Prosumer S.L." data-oe-source-id="648" data-oe-id="650" data-oe-model="ir.ui.view" data-oe-field="arch" data-oe-xpath="/data/xpath/a/img[1]"/>
                      </h4>
                      <span style="color:white; font-align:justify"> Prosumer es un portal que administra servicios profesionales que podrán ayudarte en tu reforma. Utiliza nuestros configurados o haznos llegar tu necesidad.</span>
                    </div>
                    <div class="col-md-2" id="info">
                      <h3 style="color: white;font-weight: bold;">NOSOTROS</h3>
                      <ul class="list-unstyled">
                        <li>
                          <a href="/" style="color: white;font-size: x-small;">INICIO</a>
                        </li>
                        <li>
                          <a href="/page/sobre-nosotros" style="color: white;font-size: x-small;">PROSUMER</a>
                        </li>
                        <li>
                          <a href="/blog/2" style="color: white;font-size: x-small;">PROYECTOS</a>
                        </li>
                        <li>
                          <a href="/page/website.contactus" style="color: white;font-size: x-small;">CONTACTO</a>
                        </li>
                      </ul>
                      <h2>
                        <a t-att-href="website.social_facebook" t-if="website.social_facebook">
                          <i class="fa fa-facebook-square"/>
                        </a>
                        <a t-att-href="website.social_twitter" t-if="website.social_twitter">
                          <i class="fa fa-twitter"/>
                        </a>
                        <a t-att-href="website.social_linkedin" t-if="website.social_linkedin">
                          <i class="fa fa-linkedin"/>
                        </a>
                        <a t-att-href="website.social_youtube" t-if="website.social_youtube">
                          <i class="fa fa-youtube-play"/>
                        </a>
                        <a t-att-href="website.social_googleplus" t-if="website.social_googleplus" rel="publisher">
                          <i class="fa fa-google-plus-square"/>
                        </a>
                        <a t-att-href="website.social_github" t-if="website.social_github">
                          <i class="fa fa-github"/>
                        </a>
                      </h2>
                    </div>
                    <div class="col-md-2" id="reformas">
                      <h3 style="color: white;font-weight: bold;">SOLUCIONES</h3>
                      <ul class="list-unstyled">
                        <li>
                          <a data-cke-saved-href="/shop" href="/shop" style="color: white;font-size: x-small;">REFORMAS</a>
                        </li>
                        <li>
                          <a data-cke-saved-href="/blog/3" href="/blog/3" style="color: white;font-size: x-small;">PRODUCTOS EFICIENTES</a>
                        </li>
                      </ul>
                    </div>
                    <div class="col-md-5">
                      <div>
                        <h3 style="color: white;font-weight: bold;">ÚLTIMOS PROYECTOS</h3>
                        <div class="row">
                          <div class="col-md-4">
                            <a href="/page/website.contactus" style="color: white;font-size: x-small;">
                              <img alt="img" src="http://www.totmampara.com/images/icono_plat.jpg" style=""/>
                            </a>
                          </div>
                          <div class="col-md-4">
                            <a href="/page/website.contactus" style="color: white;font-size: x-small;">
                              <img alt="img" src="http://www.totmampara.com/images/icono_plat.jpg" style=""/>
                            </a>
                          </div>
                          <div class="col-md-4">
                            <a href="/page/website.contactus" style="color: white;font-size: x-small;">
                              <img alt="img" src="http://www.totmampara.com/images/icono_plat.jpg" style=""/>
                            </a>
                          </div>
                        </div>
                        <div class="row">
                          <div class="col-md-4">
                            <a href="/page/website.contactus" style="color: white;font-size: x-small;">
                              <img alt="img" src="http://www.totmampara.com/images/icono_plat.jpg" style=""/>
                            </a>
                          </div>
                          <div class="col-md-4">
                            <a href="/page/website.contactus" style="color: white;font-size: x-small;">
                              <img alt="img" src="http://www.totmampara.com/images/icono_plat.jpg" style=""/>
                            </a>
                          </div>
                          <div class="col-md-4">
                            <a href="/page/website.contactus" style="color: white;font-size: x-small;">
                              <img alt="img" src="http://www.totmampara.com/images/icono_plat.jpg" style=""/>
                            </a>
                          </div>
                        </div>
                      </div>
                      <ul class="list-inline js_language_selector mt16" t-if="(request.website_multilang and len(languages) &gt; 1) or editable">
                        <li t-foreach="languages" t-as="lg">
                          <a style="color: white;" t-att-href="url_for(request.httprequest.path + '?' + keep_query(), lang=lg[0])" t-att-data-default-lang="editable and 'true' if lg[0] == website.default_lang_code else None" t-att-data-lang="lg[0]" class="js_change_lang ">
                            <t t-esc="lg[1].split('/').pop()"/>
                          </a>
                        </li>
                        <li groups="base.group_website_publisher">
                          <t t-set="url_return" t-value="url_for('', '[lang]') + '?' + keep_query()"/>
                          <a t-attf-href="/web#action=base.action_view_base_language_install&amp;website_id=#{website.id}&amp;url_return=#{url_return}" style="color: white;"><i class="fa fa-plus-circle"/>
                                        Add a language...
                          </a>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
            </xpath>
        </template>

        <template id="website_layout_footer_custom" inherit_id="website.layout_footer_copyright">
            <xpath expr="///div[@class='container mt16 mb8']" position="replace">
                <div class="container mt16 mb8" style="background-color: #41b6ab; color: white;width: 100%;padding:10px">
                    <div class="row">
                        <div class="pull-left text-muted">
                            Copyright <span t-field="res_company.name">Company name</span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="pull-right" style="margin-right: 10px;">
                            Proyecto cofinanciado por:
                            <img src="data:image/png;base64,"/>

                            <img src="data:image/jpeg;base64,"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </template>


        <template id="display_anoymous_prosumer_customs" inherit_id="website_anonymous_hide_prices_customs.display_anoymous">
            <xpath expr="//div[@class='jumbotron mt16 mb16']" position="replace">
                <div t-if="user_id.partner_id.name == 'Public user'" class="mt16 mb16" data-oe-model="ir.ui.view" data-oe-field="arch" data-oe-xpath="/t[1]/t[1]/div[1]/div[1]" style="background-color: #41b6ab;color: white;">
                    <section class="mb0 mt4">
                        <div class="container">
                            <h2>Bienvenido al Comercio Electrónico de prosumer.es</h2>
                            <p>Desde aquí podrás ver los productos y configuradores que hemos diseñado para tí.</p>
                            <p>Para poder ofrecerte este servicio, guardar tus presupuestos de forma que los puedas recuperar en cualquier momento y mantenerte informado necesitamos que te registres.</p>
                            <p>Si decides no hacerlo en este momento, no verás los precios y otra información de interés.</p>
                            <p>
                                <a class="btn btn-primary btn-lg" data-cke-saved-href="/web/login" href="/web/login">Acceder como usuario</a>
                            </p>
                        </div>
                    </section>
                </div>
            </xpath>
        </template>

        <template id="hide_variant_plus_price" inherit_id="website_sale.variants">
            <xpath expr="/t" position="replace">
                <t t-name="website_sale.variants">
                  <t t-set="attribute_value_ids" t-value="get_attribute_value_ids(product)"/>
                  <ol t-attf-class="list-unstyled js_add_cart_variants #{ul_class}" t-att-data-attribute_value_ids="attribute_value_ids">
                    <t t-foreach="product.attribute_line_ids" t-as="variant_id">
                      <li t-if="len(variant_id.value_ids) &gt; 1">

                        <strong t-field="variant_id.attribute_id.name"/>

                        <t t-if="variant_id.attribute_id.type in ['select', 'hidden']">
                            <select class="form-control js_variant_change_ic" t-att-name="'attribute-%s-%s' % (product.id, variant_id.attribute_id.id)">
                            <t t-foreach="variant_id.value_ids" t-as="value_id">
                              <option t-att-value="value_id.id">
                                  <span t-field="value_id.name"/>
                                  <!--<span t-if="value_id.price_extra">
                                      <t t-esc="value_id.price_extra &gt; 0 and '+' or ''"/><span t-field="value_id.price_extra" style="white-space: nowrap;" t-field-options="{                                &quot;widget&quot;: &quot;monetary&quot;,                                 &quot;from_currency&quot;: &quot;product.company_id.currency_id&quot;,                                &quot;display_currency&quot;: &quot;user_id.partner_id.property_product_pricelist.currency_id&quot;                            }"/>
                                  </span>-->
                              </option>
                            </t>
                          </select>
                        </t>

                        <t t-if="variant_id.attribute_id.type == 'radio'">
                          <ul class="list-unstyled">
                              <t t-set="inc" t-value="0"/>
                              <t t-foreach="variant_id.value_ids" t-as="value_id">
                                  <li class="form-group js_attribute_value" style="margin: 0;">
                                      <label class="control-label" style="margin: 0 20px;">
                                          <input type="radio" class="js_variant_change_ic" t-att-checked="'checked' if not inc else ''" t-att-name="'attribute-%s-%s' % (product.id, variant_id.attribute_id.id)" t-att-value="value_id.id" style="vertical-align: top; margin-right: 10px;"/>
                                          <span t-field="value_id.name"/>
                                          <!--<span class="badge" t-if="value_id.price_extra">
                                              <t t-esc="value_id.price_extra &gt; 0 and '+' or ''"/><span t-field="value_id.price_extra" style="white-space: nowrap;" t-field-options="{                                       &quot;widget&quot;: &quot;monetary&quot;,                                       &quot;from_currency&quot;: &quot;product.company_id.currency_id&quot;,                                       &quot;display_currency&quot;: &quot;user_id.partner_id.property_product_pricelist.currency_id&quot;                                    }"/>
                                          </span>-->
                                      </label>
                                  </li>
                                  <t t-set="inc" t-value="inc+1"/>
                              </t>
                          </ul>
                        </t>

                        <t t-if="variant_id.attribute_id.type == 'color'">
                          <ul class="list-inline">
                              <t t-set="inc" t-value="0"/>
                              <li t-foreach="variant_id.value_ids" t-as="value_id">
                                  <label t-attf-style="background-color:#{value_id.color or value_id.name}" t-attf-class="css_attribute_color #{'active' if not inc else ''}">
                                      <input type="radio" class="js_variant_change_ic" t-att-checked="'checked' if not inc else ''" t-att-name="'attribute-%s-%s' % (product.id, variant_id.attribute_id.id)" t-att-value="value_id.id" t-att-title="value_id.name"/>
                                  </label>
                                  <t t-set="inc" t-value="inc+1"/>
                              </li>
                          </ul>
                        </t>

                      </li>
                    </t>
                  </ol>
                </t>
            </xpath>
        </template>


    </data>
</openerp>