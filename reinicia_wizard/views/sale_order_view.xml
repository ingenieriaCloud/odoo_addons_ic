<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

      <record id="view_order_form_new_wizard" model="ir.ui.view">
        <field name="name">sale.view.order.form.new_wizard</field>
	<field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
	  <xpath expr="//field[@name='order_line']" position="before">
	    
            <button name="%(action_sale_order_line_new_wizard)d" type="action" string="Crear desde quants" states="draft,sent" />
	    <button name="%(action_sale_order_new_trazabilidad_wizard)d" type="action" string="Crear Trazabilidad" states="draft,sent" />
 	    
	  </xpath>
        </field>
      </record>

      
    </data>
</openerp>
