<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

      <record id="purchase_order_form_reinicia" model="ir.ui.view">
            <field name="name">purchase.order.form</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>

            <field name="arch" type="xml">
	      <xpath expr="//field[@name='journal_id']" position="after">
		<field name="transportista_ent" domain="[('supplier','=', True)]" context="{'search_default_supplier':1, 'default_supplier':1, 'default_customer':0}" attrs="{'readonly': [('state','not in',('draft'))]}"/>
		<field name="matricula_ent" attrs="{'readonly': [('state','not in',('draft'))]}" />
	      </xpath>
	    </field>

	  </record>
      
    </data>
</openerp>
